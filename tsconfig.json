{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "removeComments": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "CommonJS",
    "baseUrl": "."
  },
  "include": [
    "src/**/*",
    "./**/*.yaml"
  ],
  "exclude": [
    "node_modules/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}

